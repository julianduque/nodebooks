{
  "name": "nodebooks",
  "description": "Interactive notebook environment for Node.js",
  "private": true,
  "version": "0.5.1",
  "repository": "https://github.com/julianduque/nodebooks",
  "keywords": [
    "node",
    "notebook",
    "typescript"
  ],
  "license": "MIT",
  "author": "Juli√°n Duque <julianduquej@gmail.com>",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @nodebooks/server dev",
    "ui:dev": "pnpm --filter @nodebooks/client dev",
    "api:dev": "pnpm --filter @nodebooks/server dev:api-only",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "pnpm --filter @nodebooks/server start"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@next/eslint-plugin-next": "^15.5.4",
    "eslint": "^9.36.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.44.1"
  },
  "type": "module",
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef"
}
