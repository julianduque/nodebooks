{
  "name": "nodebooks",
  "description": "Interactive notebook environment for Node.js",
  "version": "0.6.0",
  "repository": "https://github.com/julianduque/nodebooks",
  "private": true,
  "keywords": [
    "node",
    "notebook",
    "typescript"
  ],
  "license": "MIT",
  "author": "Juli√°n Duque <julianduquej@gmail.com>",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "prepack": "pnpm -r build",
    "prepublishOnly": "pnpm -r build",
    "dev": "pnpm --filter @nodebooks/server dev",
    "ui:dev": "pnpm --filter @nodebooks/client dev",
    "api:dev": "pnpm --filter @nodebooks/server dev:api-only",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "pnpm --filter @nodebooks/server start"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.37.0",
    "@next/eslint-plugin-next": "^15.5.5",
    "eslint": "^9.37.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.46.1"
  },
  "type": "module",
  "packageManager": "pnpm@10.18.3+sha512.bbd16e6d7286fd7e01f6b3c0b3c932cda2965c06a908328f74663f10a9aea51f1129eea615134bf992831b009eabe167ecb7008b597f40ff9bc75946aadfb08d"
}
