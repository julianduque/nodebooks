{
  "name": "nodebooks",
  "description": "Interactive notebook environment for Node.js",
  "version": "0.6.0",
  "repository": "https://github.com/julianduque/nodebooks",
  "private": true,
  "keywords": [
    "node",
    "notebook",
    "typescript"
  ],
  "license": "MIT",
  "author": "Juli√°n Duque <julianduquej@gmail.com>",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm --filter \"@nodebooks/*\" --filter \"!@nodebooks/server\" build && pnpm --filter @nodebooks/server build",
    "prepack": "pnpm -r build",
    "prepublishOnly": "pnpm -r build",
    "dev": "pnpm --filter @nodebooks/server dev",
    "ui:dev": "pnpm --filter @nodebooks/client dev",
    "api:dev": "pnpm --filter @nodebooks/server dev:api-only",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "pnpm --filter @nodebooks/server start"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "^16.0.7",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.48.1"
  },
  "type": "module",
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
